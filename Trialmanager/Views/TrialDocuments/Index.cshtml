@model IEnumerable<Trialmanager.Models.TrialDocumentsModels>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ShortName.ShortName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeName.TypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UploadedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DocumentLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DocumentVersion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DocumentDescription)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {

        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ShortName.ShortName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeName.TypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UploadedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentVersion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DocumentDescription)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>

    }
</tbody>
</table>
</body>
</html>
