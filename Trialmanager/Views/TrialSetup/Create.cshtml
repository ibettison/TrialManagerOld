@model Trialmanager.Models.TrialSetupModels



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            @ViewBag.Title
            <small>@ViewBag.Small</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="/Home/Index"><i class="fa fa-dashboard"></i>@ViewBag.link</a></li>
            <li class="active">@ViewBag.Title</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content container-fluid">
        <div class="row">
            <div class="col-md-6">
                <div class="box box-solid box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Add a New Trial</h3>
                        <div class="box-tools pull-right">
                            <!-- Buttons, labels, and many other things can be placed here! -->
                            <!-- Here is a label for example -->
                            <span class="label label-default">Creating New Trial...</span>
                        </div>
                        <!-- /.box-tools -->
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        @using (Html.BeginForm()) 
                        {
                            @Html.AntiForgeryToken()
        
                            <div class="form-horizontal">
                                <h4>TrialSetupModels</h4>
                                <hr />
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                <div class="form-group">
                                    @Html.LabelFor(model => model.GrantFunderRef, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.GrantFunderRef, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.GrantFunderRef, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.SponsorRef, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.SponsorRef, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.SponsorRef, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.RecRef, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.RecRef, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.RecRef, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.EudractRef, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.EudractRef, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.EudractRef, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.IrasId, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.IrasId, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.IrasId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.RecruitmentTarget, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.RecruitmentTarget, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.RecruitmentTarget, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.TrialLocationId, "TrialLocationId", htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.DropDownList("TrialLocationId", null, htmlAttributes: new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.TrialLocationId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.RecDate, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.RecDate, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.RecDate, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.HraDate, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.HraDate, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.HraDate, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.CtaDate, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.CtaDate, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.CtaDate, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    @Html.LabelFor(model => model.TrialId, "TrialId", htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.DropDownList("TrialId", null, htmlAttributes: new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.TrialId, "", new { @class = "text-danger" })
                                    </div>
                                </div>
    
                                <div class="form-group">
                                    <div class="col-md-offset-2 col-md-10">
                                        <input type="submit" value="Create" class="btn btn-default" />
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer">
                        <!--Use this to keep the data last typed in a cookie so that it can be saved properly later-->
                        <p>Save what you have typed in a cookie if you are distracted!</p>
                        <a class="btn btn-app"><i class="fa fa-save"></i>Keep</a> <a class="btn btn-app"><i class="fa fa-exclamation"></i>Clear</a> <a class="btn btn-app"><i class="fa fa-close"></i>Delete</a>
                    </div>
                    <!-- box-footer -->
                </div>
                <!-- /.box -->
            </div>
        </div>
    </section>
</div>
    
    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
</body>
</html>
